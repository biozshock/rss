{
    "require": {
        "php": "^7.4",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-pdo": "*",
        "guzzlehttp/guzzle": "^7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Biozshock\\Rss\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Biozshock\\Rss\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs-fix": "./vendor/bin/php-cs-fixer fix --config .php-cs-fixer.php --using-cache no --show-progress dots -v"
    }
}
